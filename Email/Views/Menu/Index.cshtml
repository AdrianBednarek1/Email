@model Email.Models.LoggedModel
@{
    ViewData["Title"] = "Index";
}
<a asp-action="SignOut">Sign Out</a>
<p>@Model.FirstName @Model.LastName</p>
<p>@Model.EmailAddress</p>
<a asp-action="SendMail">Send Mail</a>
<table class="table" style="width:100%">
    @foreach (var item in Model.Mails)
    {
        <tr>
            <td>
                <p>@item.Name  </p>
            </td> 
            <td>
                <p>@item.Subject - @item.Message</p>
            </td>
                <td style="width:20%">
                <p>@item.DateTime_</p>
            </td>
        </tr>         
    }
</table>

